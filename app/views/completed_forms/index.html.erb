
<div class="container">

  <%= render :partial => 'welcome/buttons' %>

  <div class="completed_form_index">
    <h2>Completed Inspections</h2>
    <table class="table">
      <thead>
      <tr>
        <th></th>
        <th>Date</th>
        <th>Time</th>
        <th>Location</th>
        <th>Employee</th>
      </tr>
      </thead>

      <% @completed_forms.each do |completed| %>
          <tr class="completed_forms">
            <%= render :partial => 'completed_forms/completed_form', locals: {completed: completed} %>
          </tr>

      <% end %>
    </table>
  </div>

  <script>var lastId = <%= @last_id %></script>






