
<div class="container">

  <div class="link_buttons">
    <h4>
      <%= link_to "Completed Forms", completed_forms_path, class: "btn btn-hg btn-primary" %>
      <%= link_to "Location Status", beacons_path, class: "btn btn-hg btn-primary" %>
      <%= link_to "View Employees", employees_path, class: "btn btn-hg btn-primary" %>
      <%= link_to "Add an Employee", new_employee_path, class: "btn btn-hg btn-primary" %>
    </h4>
  </div>

<div class="completed_form_totals">
  <table class="table">
    <thead>
      <tr>
        <th></th>
        <th>Today</th>
        <th>Week</th>
        <th>Month</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <th><%= @forms.find(1).title %></th>
        <th><%= @completed_forms.today(1) %></th>
        <th><%= @completed_forms.this_week(1) %></th>
        <th><%= @completed_forms.this_month(1) %></th>
      </tr>
      <tr>
        <th><%= @forms.find(2).title %></th>
        <th><%= @completed_forms.today(2) %></th>
        <th><%= @completed_forms.this_week(2) %></th>
        <th><%= @completed_forms.this_month(2) %></th>
      </tr>
      <tr>
        <th><%= @forms.find(3).title %></th>
        <th><%= @completed_forms.today(3) %></th>
        <th><%= @completed_forms.this_week(3) %></th>
        <th><%= @completed_forms.this_month(3) %></th>
      </tr>
      <tr>
        <th>Total</th>
        <th><%= @completed_forms.total_today %></th>
        <th><%= @completed_forms.total_this_week %></th>
        <th><%= @completed_forms.total_this_month %></th>
      </tr>
    </tbody>
  </table>
</div>

  <div class="completed_form_index">
    <h2>Completed Forms</h2>
    <table class="table">
      <thead>
      <tr>
        <th></th>
        <th>Date</th>
        <th>Time</th>
        <th>Location</th>
        <th>Employee</th>
      </tr>
      </thead>

      <div class="completed_form_box">
      <% @completed_forms.each do |completed| %>
        <div class="completed_box">
          <tr class="completed_forms">
            <%= render :partial => 'completed_forms/completed_form', locals: {completed: completed} %>
          </tr>
        </div>
      <% end %>
      </div>
    </table>
  </div>

